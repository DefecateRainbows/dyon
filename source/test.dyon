    println("")
    println(cx(a))
    println(cy(a))
    println(cz(a))
    println(cw(a))
    println("")
    for i 4 {println(cv(a, i))}

    rot := \(ang) = mat4 {cos(ang), -sin(ang); sin(ang), cos(ang), 0, 0}
    println(rot)

    println(mat4 {yx (2, 1),;})
